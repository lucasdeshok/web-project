<script type="text/javascript" src="~/Scripts/jquery-3.4.1.js"></script>
@model IList<WebProject.Models.Usuario>
<table class="table table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Email</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
        <tr>
            <td>@usuario.Id</td>            
            <td>@Html.ActionLink(usuario.Nome, "Visualizar", new { id = usuario.Id })</td>
            <td>@usuario.Email</td>
            <td><a href="#" onclick="excluir(@usuario.Id)">Excluir</a></td>
        </tr>
        }
    </tbody>
</table>